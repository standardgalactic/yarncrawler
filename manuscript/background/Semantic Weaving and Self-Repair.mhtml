import numpy as np
import matplotlib.pyplot as plt

def stigmergic_growth(alpha=2.0, theta=1.0, K=50, delta=0.05, 
                      Phi0=0.1, T=200, dt=0.1):
    """
    Simulate stigmergic growth dynamics:
        dPhi/dt = alpha * (Phi/(Phi+theta)) * (1 - Phi/K) - delta*Phi
    """
    steps = int(T/dt)
    Phi = np.zeros(steps)
    Phi[0] = Phi0
    time = np.linspace(0, T, steps)

    for t in range(steps-1):
        Phi[t+1] = Phi[t] + dt * (
            alpha * (Phi[t] / (Phi[t] + theta)) * (1 - Phi[t]/K) - delta*Phi[t]
        )
        # enforce non-negativity
        Phi[t+1] = max(Phi[t+1], 0)

    return time, Phi

